PREFIX  qa:   <http://www.wdaqua.eu/qa#>
PREFIX  xsd:  <http://www.w3.org/2001/XMLSchema#>
PREFIX  oa:   <http://www.w3.org/ns/openannotation/core/>

SELECT  ?uri
FROM <urn:graph>
WHERE
  { ?a    a             qa:AnnotationOfRelation ;
          oa:hasTarget  _:b0 .
    _:b0  a             oa:SpecificResource ;
          oa:hasSource  ?q .
    ?a    oa:hasBody    ?uri
  }
